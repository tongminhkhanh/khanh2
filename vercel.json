{
    "version": 2,
    "regions": [
        "sin1"
    ],
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/server.js"
        }
    ],
    "headers": [
        {
            "source": "/(.*).(jpg|jpeg|png|gif|webp|css|js|svg)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}